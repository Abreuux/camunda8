<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="lead_enrichment_process" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.15.0">
  <bpmn:process id="lead-enrichment" name="Lead Enrichment Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Receive Lead Data">
      <bpmn:extensionElements>
        <zeebe:formDefinition formKey="camunda-forms:bpmn:userTaskForm_1" />
        <zeebe:properties>
          <zeebe:property name="formMapping" value="embedded:app:forms/start-form" />
        </zeebe:properties>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_ValidateLead" />
    
    <bpmn:serviceTask id="Task_ValidateLead" name="Validate Lead Data">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="validate-lead" />
        <zeebe:ioMapping>
          <zeebe:input source="=leadName" target="leadName" />
          <zeebe:input source="=email" target="email" />
          <zeebe:input source="=company" target="company" />
          <zeebe:output source="=leadValid" target="leadValid" />
          <zeebe:output source="=validationMessage" target="validationMessage" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ValidateLead" targetRef="Gateway_LeadValid" />
    
    <bpmn:exclusiveGateway id="Gateway_LeadValid" name="Lead Valid?" default="Flow_Invalid">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_Valid</bpmn:outgoing>
      <bpmn:outgoing>Flow_Invalid</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    
    <bpmn:sequenceFlow id="Flow_Valid" name="Yes" sourceRef="Gateway_LeadValid" targetRef="Task_EnrichLead">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=leadValid = true</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:sequenceFlow id="Flow_Invalid" name="No" sourceRef="Gateway_LeadValid" targetRef="EndEvent_Invalid">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=leadValid = false</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    
    <bpmn:serviceTask id="Task_EnrichLead" name="Enrich Lead Data">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="lead-enrichment" />
        <zeebe:ioMapping>
          <zeebe:input source="=leadName" target="leadName" />
          <zeebe:input source="=email" target="email" />
          <zeebe:input source="=company" target="company" />
          <zeebe:output source="=enrichedData" target="enrichedData" />
          <zeebe:output source="=linkedinData" target="linkedinData" />
          <zeebe:output source="=companyData" target="companyData" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Valid</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_EnrichLead" targetRef="Task_StoreResults" />
    
    <bpmn:serviceTask id="Task_StoreResults" name="Store Enriched Data">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="store-lead" />
        <zeebe:ioMapping>
          <zeebe:input source="=enrichedData" target="enrichedData" />
          <zeebe:input source="=linkedinData" target="linkedinData" />
          <zeebe:input source="=companyData" target="companyData" />
          <zeebe:output source="=storageSuccess" target="storageSuccess" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_StoreResults" targetRef="Task_NotifySuccess" />
    
    <bpmn:serviceTask id="Task_NotifySuccess" name="Send Success Notification">
      <bpmn:extensionElements>
        <zeebe:taskDefinition type="notify-success" />
        <zeebe:ioMapping>
          <zeebe:input source="=leadName" target="leadName" />
          <zeebe:input source="=enrichedData" target="enrichedData" />
          <zeebe:output source="=notificationSent" target="notificationSent" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_NotifySuccess" targetRef="EndEvent_Success" />
    
    <bpmn:endEvent id="EndEvent_Success" name="Lead Enrichment Complete">
      <bpmn:incoming>Flow_6</bpmn:incoming>
    </bpmn:endEvent>
    
    <bpmn:endEvent id="EndEvent_Invalid" name="Invalid Lead">
      <bpmn:incoming>Flow_Invalid</bpmn:incoming>
    </bpmn:endEvent>
  </bpmn:process>
  
  <bpmndi:BPMNDiagram id="BPMNDiagram_1">
    <bpmndi:BPMNPlane id="BPMNPlane_1" bpmnElement="lead-enrichment">
      <!-- Start Event -->
      <bpmndi:BPMNShape id="StartEvent_1_di" bpmnElement="StartEvent_1">
        <dc:Bounds x="152" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="134" y="145" width="73" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Validate Lead Task -->
      <bpmndi:BPMNShape id="Task_ValidateLead_di" bpmnElement="Task_ValidateLead">
        <dc:Bounds x="240" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Gateway -->
      <bpmndi:BPMNShape id="Gateway_LeadValid_di" bpmnElement="Gateway_LeadValid" isMarkerVisible="true">
        <dc:Bounds x="395" y="95" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="389" y="65" width="61" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Enrich Lead Task -->
      <bpmndi:BPMNShape id="Task_EnrichLead_di" bpmnElement="Task_EnrichLead">
        <dc:Bounds x="500" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Store Results Task -->
      <bpmndi:BPMNShape id="Task_StoreResults_di" bpmnElement="Task_StoreResults">
        <dc:Bounds x="660" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Notify Success Task -->
      <bpmndi:BPMNShape id="Task_NotifySuccess_di" bpmnElement="Task_NotifySuccess">
        <dc:Bounds x="820" y="80" width="100" height="80" />
      </bpmndi:BPMNShape>

      <!-- Success End Event -->
      <bpmndi:BPMNShape id="EndEvent_Success_di" bpmnElement="EndEvent_Success">
        <dc:Bounds x="982" y="102" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="960" y="145" width="81" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Invalid End Event -->
      <bpmndi:BPMNShape id="EndEvent_Invalid_di" bpmnElement="EndEvent_Invalid">
        <dc:Bounds x="402" y="232" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="391" y="275" width="58" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>

      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="Flow_1_di" bpmnElement="Flow_1">
        <di:waypoint x="188" y="120" />
        <di:waypoint x="240" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_2_di" bpmnElement="Flow_2">
        <di:waypoint x="340" y="120" />
        <di:waypoint x="395" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Valid_di" bpmnElement="Flow_Valid">
        <di:waypoint x="445" y="120" />
        <di:waypoint x="500" y="120" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="464" y="102" width="18" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_Invalid_di" bpmnElement="Flow_Invalid">
        <di:waypoint x="420" y="145" />
        <di:waypoint x="420" y="232" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="428" y="186" width="15" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_4_di" bpmnElement="Flow_4">
        <di:waypoint x="600" y="120" />
        <di:waypoint x="660" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_5_di" bpmnElement="Flow_5">
        <di:waypoint x="760" y="120" />
        <di:waypoint x="820" y="120" />
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_6_di" bpmnElement="Flow_6">
        <di:waypoint x="920" y="120" />
        <di:waypoint x="982" y="120" />
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions> 